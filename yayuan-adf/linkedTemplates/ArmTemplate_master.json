{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "yayuan-adf"
		},
		"AzureMLService1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "4aaa645c-5ae2-4ae9-a17a-84b9023bc56a"
		},
		"AzureMLService1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "yayuan"
		},
		"AzureMLService1_properties_typeProperties_mlWorkspaceName": {
			"type": "string",
			"defaultValue": "ocp-ws"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "yayuan-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureMLService1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureMLService1_properties_typeProperties_subscriptionId')]"
					},
					"AzureMLService1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureMLService1_properties_typeProperties_resourceGroupName')]"
					},
					"AzureMLService1_properties_typeProperties_mlWorkspaceName": {
						"value": "[parameters('AzureMLService1_properties_typeProperties_mlWorkspaceName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}